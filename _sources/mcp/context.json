{

    "@context": {
      "@version": 1.1,
      "@vocab": "http://modelcontextprotocol.io/ontology#",
      "mcp": "http://modelcontextprotocol.io/ontology#",
      "jsonrpc": "http://json-rpc.org/ontology#",
      "xsd": "http://www.w3.org/2001/XMLSchema#",
      "rdfs": "http://www.w3.org/2000/01/rdf-schema#",

      "jsonrpc": {
        "@id": "jsonrpc:version",
        "@type": "xsd:string"
      },
      "id": {
        "@id": "mcp:hasID",
        "@type": "@id"
      },
      "method": {
        "@id": "mcp:hasMethod",
        "@type": "xsd:string"
      },
      "params": {
        "@id": "mcp:hasParameters"
      },
      "result": {
        "@id": "mcp:hasResult"
      },
      "error": {
        "@id": "mcp:hasError",
        "@type": "@id"
      }
    }
  }
